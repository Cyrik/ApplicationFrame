{
  "name": "application-frame",
  "version": "0.4.0",
  "description": "Application Frame is a small JavaScript Framework / Library at a realy early stage.",
  "main": "af.js",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "merge-stream": "^1.0.0",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha .travis/main.js",
    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- .travis/main.js --report progress",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "jsdoc": "jsdoc af.js core/ modules/ util/ -r -p -a all -P package.json -c jsdoc.conf.json -t node_modules/ink-docstrap/template"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TitanNanoDE/ApplicationFrame.git"
  },
  "keywords": [
    "framework",
    "library",
    "application-frame"
  ],
